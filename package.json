    "migration:revert": "tsc && node_modules/.bin/typeorm migration:revert"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^17.4.0",
    "lodash": "^4.17.21",
    "pg": "^8.6.0",
    "redis": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.168",
    "@types/redis": "^2.8.28",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
